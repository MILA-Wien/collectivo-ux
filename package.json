{
  "name": "machmit",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173 --host",
    "test:unit": "vitest --environment jsdom --coverage",
    "test:e2e": "start-server-and-test preview http://collectivo.local:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://collectivo.local:4173/ 'cypress run --e2e'",
    "test:coverage": "vitest run --coverage",
    "build-only": "vite build",
    "build:staging": "vite build --mode staging",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@vuelidate/core": "^2.0.0",
    "@vuelidate/validators": "^2.0.0",
    "axios": "^1.1.2",
    "keycloak-js": "^20.0.1",
    "mathjs": "^11.4.0",
    "pinia": "^2.0.23",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primevue": "^3.17.0",
    "sass": "^1.55.0",
    "vue": "^3.2.40",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.5",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2"
  },
  "devDependencies": {
    "@originjs/vite-plugin-federation": "^1.1.9",
    "@pinia/testing": "^0.0.14",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.8.3",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitest/coverage-c8": "^0.25.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^11.1.0",
    "cypress-keycloak": "^2.0.0",
    "eslint": "^8.25.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.6.0",
    "jsdom": "^20.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.7",
    "vite-plugin-top-level-await": "^1.1.1",
    "vitest": "^0.25.2",
    "vue-tsc": "^1.0.3",
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.7",
    "tailwindcss": "^3.2.4"
  }
}
